            <property name="queryText">select ORGANISATION.ORG_ID
, ORGANISATION.ORG_MATCHCODE
, ACTIVITY.ACT_ORGID, PROJECT.PROJ_NAME
, (select sel_value
from selectiontables
where sel_ID like 'act_type'
and sel_languser = ?
and sel_subitem = ACTIVITY.ACT_TYPE) as ActivityType
from ACTIVITY, ORGANISATION, PROJECT
where ORGANISATION.ORG_ID = ACTIVITY.ACT_ORGID
and ORGANISATION.ORG_ID = PROJECT.PROJ_ORGID
and not PROJECT.PROJ_NAME = ''
</property>
