            <property name="queryText">select  odo.OD_PRODUCTCODE
, odo.OD_NUMBER
, (select PP_UNITOFMEASURE
   from dbo.PROD_PRICE
   where PP_TYPE = (select ORG_PRODPRICECAT
				    from dbo.ORGANISATION_ADDRESS
                    where ORG_ID = oh.ORDERH_ORGID
                    and ORG_VERSIONSTATUS = 'primary'
                    and ADD_ISPRIMARY = 1)
   and PP_PRODID = odo.OD_PRODUCTID) UnitsOfMeasure
, odo.OD_DESCRIPTION
, (select sel_value
   from dbo.SELECTIONTABLES
   where sel_id = 'workflowstatus'
   and sel_languser = ?
   and sel_subitem = OD_STATUS) Status
, convert(nvarchar,OD_DISCOUNT)+ ' ' +OD_DISCOUNTTYPE
, odo.OD_UNITPRICE
, convert(nvarchar,odo.OD_VATPERC) +'%'
, (select 
   case od.OD_DISCOUNTTYPE
   when '%' then 
			((od.OD_VALUE)* (1-(od.OD_DISCOUNT / 100)))
   when '€' then
			((od.OD_VALUE) - od.OD_DISCOUNT)
   else 
			(od.OD_VALUE)
   end
   from dbo.ORDERDETAIL od
   where od.OD_ID = odo.OD_ID and od.OD_ISACTIVE = 1)PriceExcl
 , (select 
   case od.OD_DISCOUNTTYPE
   when '%' then 
			((od.OD_VALUE * ((od.OD_VATPERC / 100)+1))* (1-(od.OD_DISCOUNT / 100)))
   when '€' then
			((od.OD_VALUE * ((od.OD_VATPERC / 100)+1)) - od.OD_DISCOUNT)
   else 
			(od.OD_VALUE * ((od.OD_VATPERC / 100)+1))
   end
   from dbo.ORDERDETAIL od
   where od.OD_ID = odo.OD_ID and od.OD_ISACTIVE = 1) PriceIncl
from dbo.ORDERDETAIL odo, dbo.ORDERHEADER oh
where oh.ORDERH_ID = odo.OD_ORDERHID
and odo.OD_ORDERHID = ?
and odo.OD_ISACTIVE = 1</property>
