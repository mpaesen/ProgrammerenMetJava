                    <expression name="expr">var DataField = dataSetRow["act_date"];
var Timespan = params["prmPeriod"];
var tester = 0;


if ( Timespan == "Week")
{
	if(DateTimeSpan.days(DataField, new Date() ) &lt;= 6)
	{
	 tester = 1;
	}
}
else if ( Timespan == "Month")
{
	if(DataField.getMonth() == (params["prmMonth"]  ) &amp;&amp; DataField.getFullYear() == params["prmYear"])
	{
		tester = 1;
	}
}
else if( Timespan == "Quarter")
{
  	if(DataField.getMonth() >= params["prmQuarter"] &amp;&amp; DataField.getMonth() &lt; (params["prmQuarter"] + 3)  &amp;&amp; DataField.getFullYear() == params["prmYear"])
   	{
   	tester = 1;
   	}
}
else if( Timespan == "Year")
{
	 if (DataField.getFullYear() == (params["prmYear"]))
	 {
	 tester = 1;
	 }
}
else
{
DataField == "null";
}

tester;</expression>
