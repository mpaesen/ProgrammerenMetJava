            <property name="queryText">select sel1.sel_value as act_type,sel2.sel_value as act_status,ORGANISATION.ORG_MATCHCODE
,act_date
from ACTIVITY,ORGANISATION,selectiontables sel1, selectiontables sel2
where ORGANISATION.ORG_ID = ACTIVITY.ACT_ORGID
and sel1.sel_id = 'act_type'
and sel1.sel_languser = ?
and sel1.sel_subitem = ACTIVITY.ACT_TYPE
and sel2.sel_id = 'act_status_campaignact'
and sel2.sel_languser = ?
and sel2.sel_subitem = ACTIVITY.ACT_STATUS</property>
